Label:
    @text
    @name
    @parent_name
    @tk
    index = 0
    
    new(text):
        .text = text
        .name = "label" + class.index.to_string()
        class.index = class.index + 1

    @get_name():
        return .parent_name + .name
    
    @do():
        if .tk == null
            return
        
        .tk.do("label", .get_name(), "-text", .text)
